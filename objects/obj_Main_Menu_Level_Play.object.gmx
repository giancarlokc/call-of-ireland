            <kind>1</kind>
            <string>/* Check if the current level is unlocked */
if visible == true and (obj_Level_Box.level_option &lt;= global.levelUpTo){
    /* Plays the click sound */
    audio_play_sound(sd_Click03,1,false);
    /* Tells the obj_Shop_Box the next level is the tutorial level and go to Shop room */
    if obj_Level_Box.level_option &gt; 1 {
        global.current_level = obj_Level_Box.level_option+1;
    } else {
        global.current_level = obj_Level_Box.level_option;
    }
    if global.current_level == 2 {
        room_goto(5);
    } else if global.current_level == 1 {
        room_goto(3);
    } else if global.current_level == 3 {
        room_goto(4);
    } else if global.current_level == 4 {
        room_goto(6);
    } else if global.current_level == 5 {
        room_goto(7);
    } else if global.current_level == 6 {
        room_goto(8);
    }
} else {
    /* Plays the click sound */
    audio_play_sound(sd_Click02,1,false);
}
</string>
