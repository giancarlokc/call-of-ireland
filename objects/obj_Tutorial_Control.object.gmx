            <string>audio_music_gain(global.music_volume, 0);

if obj_Builder.phy_position_y &gt; 720 or obj_Jumper.phy_position_y &gt; 720 or obj_Timy.phy_position_y &gt; 720 {
    obj_Builder.phy_position_x = checkpoint_x;
    obj_Builder.phy_position_y = checkpoint_y;
    obj_Jumper.phy_position_x = checkpoint_x;
    obj_Jumper.phy_position_y = checkpoint_y;
    obj_Timy.phy_position_x = checkpoint_x;
    obj_Timy.phy_position_y = checkpoint_y;
}

if too_far == true {
    obj_Builder.phy_position_x = checkpoint_x;
    obj_Builder.phy_position_y = checkpoint_y;
    obj_Jumper.phy_position_x = checkpoint_x;
    obj_Jumper.phy_position_y = checkpoint_y;
    obj_Timy.phy_position_x = checkpoint_x;
    obj_Timy.phy_position_y = checkpoint_y;
    too_far = false;
}

if(global.party_size == 0){
    global.n_Builders = 1;
    global.n_Jumpers = 1;
    global.n_Timys = 1;
    global.party_size = 3;
    room_goto(4);   
}
    
if build_pause == true {
    physics_pause_enable(true);
    obj_Build_Tutorial.x = view_xview[0] + 700;
    obj_Build_Tutorial.y = 400;
    obj_Build_Tutorial.visible = true;
    obj_Build_Tutorial.image_speed = 1;
    obj_Timy.paused = true;
    
    
    if build_pause_finished == true {
        obj_Build_Tutorial.visible = false;
        obj_Build_Tutorial.image_speed = 0;
        physics_pause_enable(false);
        obj_Timy.paused = false;
    }
}

if timy_pause == true {
    physics_pause_enable(true);
    obj_Timy.paused = true;
    
    if timy_pause_finished == true {
        physics_pause_enable(false);
        obj_Timy.paused = false;
    }
}
