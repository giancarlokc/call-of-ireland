            <kind>1</kind>
            <string>/* If all characters died, goes back to main menu */
global.party_size = 3
global.n_Builders = 1;
global.n_Jumpers = 1;
global.n_Timys = 1;

/* Maximum number of tables that can be built */
max_n_tables = 100;
max_n_pauses = 100;  

/* The score the player wins finishing the level */
level_score = 50;

too_far = false;
    
build_pause = false;
build_pause_finished = false;

timy_pause = false;
timy_pause_finished = false;

checkpoint_x = 0;
checkpoint_y = 0;

instance_create(0,0,obj_Cook);

instance_create(0,0,obj_Build_Tutorial);
obj_Build_Tutorial.visible = false;
obj_Build_Tutorial.image_speed = 0;

audio_stop_music();
//audio_play_music(sd_Tutorial, true);
/* Set the spawn coordinates for the characters */
spawn_x = 70;
spawn_y = 340;

checkpoint_x = spawn_x;
checkpoint_y = spawn_y;

/* Create the characters */
for(i =0; i&lt;global.n_Timys;i++){
    instance_create(spawn_x, spawn_y, obj_Timy);
    global.leader = obj_Timy;
}
for(i =0; i&lt;global.n_Jumpers;i++){
    instance_create(spawn_x, spawn_y, obj_Jumper);
    global.leader = obj_Jumper;
}
for(i =0; i&lt;global.n_Builders;i++){
    instance_create(spawn_x, spawn_y, obj_Builder);
    global.leader = obj_Builder;
}
obj_Brick.depth = -9000;
if(global.n_Builders &gt; 0)
    obj_Builder.depth = -9999;
if(global.n_Jumpers &gt; 0)
    obj_Jumper.depth = - 9998;
if(global.n_Timys &gt; 0)
    obj_Timy.depth = -9997;
</string>
